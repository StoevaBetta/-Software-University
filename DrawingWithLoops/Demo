public class Demo{
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        for (int i = 0; i < 4; i++){
            System.out.printf("i = %d%n", i);
            for (int m = 0; m < 5; m++){
                System.out.printf("m = %d", m);

            }
            System.out.println();
        }

        }
    }
