import java.util.Scanner;

/**
 * Created by gerganastoeva on 7/28/17.
 */


class ForLoopExample {
    public static void main(String args[]){
         Scanner scanner = new Scanner(System.in);
         
         for(int i=10; i>1; i--){
              System.out.println("The value of i is: "+i);
         }
    }
}
